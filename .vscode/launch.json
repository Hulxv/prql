  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'prql'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--lib",
          "--package=prql"
        ],
        "filter": {
          "name": "prql",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'prql'",
      "cargo": {
        "args": [
          "build",
          "--bin=prql",
          "--package=prql"
        ],
        "filter": {
          "name": "prql",
          "kind": "bin"
        }
      },
      "args": [
        "compile",
        "a.prql"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'prql'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=prql",
          "--package=prql"
        ],
        "filter": {
          "name": "prql",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}